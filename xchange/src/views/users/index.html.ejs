<div class="index container">
  <h2 class="title">Users</h2>
  <% if (!usersList.length) { %>
  <p>No results</p>
  <% } else { %>
  <table class="display" id="generic-list">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
      </tr>
    </thead>
    <tbody>
      <% usersList.forEach((user) => { %>
      <tr>
        <td><%- user.firstName %></td>
        <td><%- user.lastName %></td>
        <td>
        <td><a href="<%- `/users/${user.id}` %>">View profile</a></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
  <% } %>
</div>